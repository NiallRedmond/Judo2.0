<h1>Log in </h1>
<%=form_tag do%>
	<table>
		<tr>
			<td> <%=label_tag :student_name%> </td>
			<td> <%=text_field_tag :student_name%>
		</tr>
		<tr>
			<td> <%=label_tag :password%> </td>
			<td> <%=password_field_tag :password%>
		</tr>
		<tr>
			<td> <%=submit_tag "Log in"%> </td>
		</tr>
	</table>
<%end%>